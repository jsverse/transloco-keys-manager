{
  "name": "@ngneat/transloco-keys-manager",
  "version": "3.8.0",
  "description": "Extract translatable keys from projects that uses Transloco",
  "engines": {
    "node": ">=18.13.0"
  },
  "type": "module",
  "exports": {
    "types": "./public-api.d.ts",
    "default": "./public-api.js"
  },
  "bin": {
    "transloco-keys-manager": "index.js"
  },
  "scripts": {
    "postinstall": "npx husky install",
    "release": "standard-version --infile ./CHANGELOG.md",
    "commit": "git-cz",
    "test": "cross-env PRODUCTION=true node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js",
    "start": "npm run prettier && tsc --watch",
    "prebuild": "npm run clean:dist",
    "build": "tsc",
    "postbuild": "node ./scripts/post-build.js",
    "clean:dist": "rimraf dist",
    "prettier": "prettier --write src/**/*.ts && prettier --write __tests__/*.ts",
    "conduct": "import-conductor",
    "pre-commit": "lint-staged"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngneat/transloco-keys-manager.git"
  },
  "keywords": [
    "angular",
    "angular 2",
    "i18n",
    "extract",
    "transloco",
    "translate",
    "keys",
    "tool",
    "cli",
    "webpack"
  ],
  "author": "Shahar Kazaz",
  "maintainers": [
    {
      "name": "Shahar Kazaz"
    },
    {
      "name": "Netanel Basal"
    }
  ],
  "bugs": {
    "url": "https://github.com/ngneat/transloco-keys-manager/issues"
  },
  "homepage": "https://github.com/ngneat/transloco-keys-manager#readme",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": [
      "import-conductor --staged",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@angular/compiler": "16.2.6",
    "@ngneat/transloco-utils": "5.0.0",
    "@phenomnomnominal/tsquery": "6.1.3",
    "chalk": "5.3.0",
    "cheerio": "1.0.0-rc.12",
    "cli-table3": "0.6.3",
    "command-line-args": "5.2.1",
    "command-line-usage": "7.0.1",
    "cosmiconfig": "8.3.6",
    "debug": "4.3.4",
    "deep-diff": "1.0.2",
    "flat": "6.0.1",
    "fs-extra": "11.1.1",
    "gettext-parser": "7.0.1",
    "glob": "10.3.7",
    "jsonc-parser": "3.2.0",
    "ora": "7.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "7.22.20",
    "@babel/preset-typescript": "7.22.15",
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-angular": "17.7.0",
    "@commitlint/config-conventional": "17.7.0",
    "@types/command-line-args": "^5.2.1",
    "@types/command-line-usage": "^5.0.2",
    "@types/debug": "4.1.9",
    "@types/deep-diff": "1.0.3",
    "@types/fs-extra": "^11.0.2",
    "@types/gettext-parser": "^4.0.2",
    "@types/glob": "^8.1.0",
    "@types/jest": "29.5.5",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "cross-env": "7.0.3",
    "git-cz": "4.9.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-preset-angular": "13.1.2",
    "lint-staged": "14.0.1",
    "lodash.isequal": "4.5.0",
    "prettier": "3.0.3",
    "rimraf": "5.0.1",
    "standard-version": "9.5.0",
    "terser": "5.20.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  }
}
